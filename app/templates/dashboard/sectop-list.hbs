{{#each articles}}
	<div class="dashboard-item">
		<div class="dashboard-item-title float-left">
			<span class="bmy-span-icon bmy-bg-color-gray1"></span>
			<span>{{BMYArticleLink this}}</span>
		</div>
		<div class="float-right">
			<span class='dashboard-item-author'><a href="#">{{author}}</a>@{{BMYBoardLink this}}</span>
			{{#if type}}
				{{BMYTime tid}}
			{{else}}
				{{BMYTime aid}}
			{{/if}}
		</div>
		<div class="clear"></div>
	</div>
{{/each}}
