{{#each articles}}
	<div class="dashboard-item">
		<div class="dashboard-item-title float-left">
			<span class="bmy-span-icon bmy-bg-color-gray1"></span>
			<span>{{bmy-article-link this}}</span>
		</div>
		<div class="float-right">
			<span class='dashboard-item-author'><a href="#">{{author}}</a>@{{bmy-board-link this}}</span>
			{{#if type}}
				{{bmy-time tid}}
			{{else}}
				{{bmy-time aid}}
			{{/if}}
		</div>
		<div class="clear"></div>
	</div>
{{/each}}
